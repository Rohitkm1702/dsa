//task is to complete the function findSum() which takes the array A[] and its size N as inputs and returns the summation of minimum and maximum element of the array.


class Solution
{ 
    public static int findSum(int A[],int N) 
    {
        //code here
        int maxi = A[0];
        int mini = A[0];
        for(int i = 0;i<N;i++)
        {
            maxi = Math.max(maxi,A[i]);
            mini = Math.min(mini,A[i]);
            
        }
        return maxi+mini;
    }
}
